                                    <th class="texto">Idade</th>
                                    <th class="texto">Endereco</th>
                                    <th class="texto">Telefone</th>
                                    <th class="texto">Foto</th>
                                    <th class="texto">Local De Trabalho</th>
                                    <th class="texto">Biografia</th>
                                    <th class="texto">Ação</th>
                                </tr>
                            </thead>
                            <tbody>
                               @foreach (var N in ViewBag.Nutricionista)
                               {
                                    <tr>
                                        <td>@(N.Nome)</td>
                                        <td>@(N.Senha)</td>
                                        <td>@(N.CPF)</td>
                                        <td>@(N.Email)</td>
                                        <td>@(N.Idade)</td>
                                        <td>@(N.Endereco)</td>
                                        <td>@(N.Telefone)</td>
                                        <td>@(N.ImagemPerfil)</td>
                                        <td>@(N.LocalTrabalho)</td>
                                        <td>@(N.Bio.Substring(0, Math.Min(N.Bio.Length, 45)) + "...")</td>
                                        <td>
                                            <a href="~/Nutricionista/ApagarN/@(N.Cod)">Apagar/</a>
                                            <a href="~/Adm/previsualizacao/@(N.Cod)">Ver</a>
                                        </td>
                                    </tr>
                               }

                            </tbody>
                        </table>
                    </section>
                </div>
            </li>
            <!--DLC-->
            <li class="tabs">
