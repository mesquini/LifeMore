                            <tbody>
                               @foreach(var A in ViewBag.Alimento) { 
                                    <tr>
                                        <td>@(A.Nome)</td>
                                        <td>@(A.Peso)</td>
                                        <td>@(A.Caloria) Kcal</td>
                                        <td>@(A.Gordura) g</td>
                                        <td>@(A.Carboidrato) g</td>
                                        <td>@(A.Proteina) g</td>
                                        <td>@(A.Categoria1)</td>
                                        <td>
                                            <a href="~/Alimentos/ApagarA/@(A.Cod)">Apagar</a>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </section>

                </div>
            </li>
