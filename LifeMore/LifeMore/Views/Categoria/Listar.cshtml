    <!--USUARIOS-->
    <div class="nav_tabs">
        <ul>
            <li class="tabs">
                <input type="button" name="tabs" class="rd_tabs" id="tab1" checked>
                <label for="tab1" id="label1"> Pacientes </label>

                <div class="content_tab" id="content_tab1">
                    <section>
                        <table border="1">
                            <thead>
                                @if (ViewBag.Error != null)
                                {
                                    <div class="alert alert-danger">
                                        <strong>ERRO!</strong> @ViewBag.Error
                                    </div>
                                }
                                <tr><a href="/Entrar/Cadastrar/">Cadastrar novo Usuário</a></tr>
                                <tr class="Produto">
                                    <th class="texto">Nome</th>
                                    <th class="texto">Senha</th>
                                    <th class="texto">CPF</th>
                                    <th class="texto">Email</th>
                                    <th class="texto">Objetivo</th>
                                    <th class="texto">Idade</th>
                                    <th class="texto">Peso</th>
                                    <th class="texto">Altura</th>
                                    <th class="texto">Endereco</th>
                                    <th class="texto">Telefone</th>
                                    <th class="texto">Foto</th>
                                    <th class="texto">Ação</th>
                                </tr>

                            </thead>
                            <tbody>
                                @foreach (var P in ViewBag.Usuario)
                                {


                                    <tr>
                                        <td>@(P.Nome)</td>
                                        <td>@(P.Senha)</td>
                                        <td>@(P.CPF)</td>
                                        <td>@(P.Email)</td>
                                        <td>
                                        @if (P.Objetivo == 1)
                                        {
                                        <p>Engordar</p> }
                                         else {
                                        <p>Emagrecer</p>}
                                        </td>
                                        <td>@(P.Idade)</td>
                                        <td>@(P.Peso)</td>
                                        <td>@(P.Altura)</td>
                                        <td>@(P.Endereco)</td>
                                        <td>@(P.Telefone)</td>
                                        <td>@(P.ImagemPerfil)</td>
                                        <td>
                                            <a href="~/Perfil/ApagarP/@(P.Cod)">Apagar/</a>
                                            <a href="~/Adm/previsualizacaoP/@(P.Cod)">Ver</a>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </section>
                </div>
