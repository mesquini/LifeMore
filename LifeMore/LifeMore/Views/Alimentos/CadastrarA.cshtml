<body class="bgEntrar">
    <div class="Main-agile">
        <!-- banner -->
        <div class="bannerEntrar">
            <div class="container">
                <div class="col-md-4 agile1">
                    <nav class="navbar navbar-default">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>

                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse nav-wil" id="bs-example-navbar-collapse-1">
                            <nav class="stroke">
                                <ul class="nav navbar-nav">
                                    <li class="active"><a href="~/Adm/Listar" style="text-shadow: 2px 1px 0px #000000">Perfil</a></li>
                                    <li><a href="~/Home/Index" style="text-shadow: 2px 1px 0px #000000">Home</a></li>
                                    <li><a href="~/Home/Dicas" style="text-shadow: 2px 1px 0px #000000">Dicas</a></li>
                                    <li><a href="~/Home/About" style="text-shadow: 2px 1px 0px #000000">Quem somos</a></li>
                                    <li><a href="~/Home/Contato" style="text-shadow: 2px 1px 0px #000000">Fale conosco</a></li>
                                    <li><a href="~/Adm/Sair" style="text-shadow: 2px 1px 0px #000000">Sair</a></li>

                                </ul>
                            </nav>
                        </div>
                        <!-- /.navbar-collapse -->
                    </nav>
                </div>
            </div>
        </div>


        <div class="message warning">
            <div class="inset agile">
                <div class="sap_tabs w3ls-tabs">
                    <div id="horizontalTab" style="display: block; width: 100%; margin: 0px;">
                        <a href="~/Adm/Listar">Voltar</a>
                        <ul class="resp-tabs-list">
                            <li class="resp-tab-item resp-tab-active" aria-controls="tab_item-1" role="tab"><a href="~/Alimentos/CadastrarA">Cadastrar Alimento</a></li>
                        </ul>
                        <div class="clear"> </div>
                        <div class="resp-tabs-container">

                            <script src="~/js/Mascara.js"></script>
                            <div class="tab-1 resp-tab-content" aria-labelledby="tab_item-1">
                                <div class="login-agileits-top sign-up">
                                    <form action="/Alimentos/CadastrarA" method="post">
                                        <p>Nome do alimento: </p>
                                        <input type="text" name="nomeA" id="nomeA" required maxlength="50" />
                                        <p>Peso:</p>
                                        <input type="text" name="pesoA" id="pesoA" required  />
                                        <p>Caloria: </p>
                                        <input type="text" name="caloriaA" id="caloriaA" required />
                                        <p>Gordura:</p>
                                        <input type="text" name="gorduraA" id="gorduraA" required/>
                                        <p>Carboidrato:</p>
                                        <input type="text" id="carboidratoA" name="carboidratoA" />
                                        <p>Proteína: </p>
                                        <input type="text" id="proteinaA" name="proteinaA" />
                                        <p>Categoria: </p>                                            
                                                <select name="categoriaA" id="categoriaA">

                                                    <option>Selecione a categoria...</option>
                @foreach (var C in ViewBag.Categoria)
{
                                                    <option value="@(C.Cod)">@(C.Nome)</option>
}
                                                </select>
                                        
                                                <input type="submit" value="Cadastrar Alimento">

                                    </form>
                                    @if (ViewBag.MsgErro != null)
                                    {
                                        <div class="alert alert-danger">
                                            <strong>ERRO!</strong> @ViewBag.MsgErro
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="right-section-w3ls"></div>
                <div class="clear"> </div>
            </div>
        </div>
        <script type="text/javascript" src="~/js/jquery-2.1.4.min.js"></script>
        <script>
                                $(document).ready(function(c) {
	                            $('.alert-close').on('click', function(c){
		                            $('.message').fadeOut('slow', function(c){
	  		                            $('.message').remove();
		                            });
	                            });
                            });
        </script>
        <script src="~/js/easyResponsiveTabs.js" type="text/javascript"></script>
        <!-- ResponsiveTabs js -->
        <script type="text/javascript">
				                $(document).ready(function () {
					                $('#horizontalTab').easyResponsiveTabs({
						                type: 'default', //Types: default, vertical, accordion
						                width: 'auto', //auto or any width like 600px
						                fit: true   // 100% fit in a container
					                });
				                });
        </script>
        <!-- //ResponsiveTabs js -->
